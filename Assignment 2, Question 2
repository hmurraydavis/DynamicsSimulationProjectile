b=1; %m
c=.248; %m
%acceleration in r hat:1.1
%acceleration in theta: 7.4

tt=135/0.0174532925; dtt=6.2831853; ddtt=0;

t1=2*c*dtt*sin(tt)*dtt; %13.8461
t2=c*((dtt^2)*cos(tt)+ddtt*sin(tt)); %-6.923
t3=(b-c*cos(tt))*ddtt; %1
t4=((b-c*cos(tt))*dtt^2); %46

athetahat=t1+t3
arhat=t2-t4


